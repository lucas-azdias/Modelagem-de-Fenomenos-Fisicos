        "\n",
        "import time\n",
        "import tracemalloc\n",
        "\n",
        "import random as rand\n",
        "\n",
        "initialTime = time.time_ns() # Salva o tempo inicial em nanosegundos\n",
        "tracemalloc.start() # Inicia monitoramento da mem贸ria\n",
        "\n",
        "chars = list()\n",
        "for i in range(1000):\n",
        "  chars.append(chr(rand.randint(0, 127)))\n",
        "# OBS.: Usando uma String acaba sendo muito mais eficiente em consumo de mem贸ria\n",
        "\n",
        "memoryUsed = tracemalloc.get_traced_memory() # Salva a mem贸ria usada\n",
        "tracemalloc.stop() # Finaliza monitoramento da mem贸ria\n",
        "endTime = time.time_ns() # Salva o tempo final em nanosegundos\n",
        "\n",
